{
  "expo": {
    "name": "GravoPlus",
    "slug": "gravoplus-management-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "backgroundColor": "#0f172a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gravoplus.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0f172a"
      },
      "package": "com.gravoplus.app"
    },
    "plugins": [
      "expo-asset",
      "expo-font"
    ]
  }
}
